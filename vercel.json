{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@vercel/static-build", "config": { "distDir": "dist" } }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "env": {
    "VITE_CONTACT_API_URL": "@vite_contact_api_url",
    "VITE_SITE_URL": "@vite_site_url",
    "VITE_ANALYTICS_ID": "@vite_analytics_id"
  }
}
